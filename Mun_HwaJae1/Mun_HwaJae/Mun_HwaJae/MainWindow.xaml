<Window x:Class="Mun_HwaJae.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mun_HwaJae"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <WrapPanel>
            <Label Content="검색" FontSize="15"/>
            <TextBox x:Name="addreTxt" Text="전라북도 전주시 " Width="200" Height="20"/>
            <Button x:Name="Click" Click="Click_Click" Width="40" Height="20" Margin="10">search</Button>
            <Button x:Name="reset" Click="Reset_Click" Width="40" Height="20" Margin="10" >reset</Button>
        </WrapPanel>
        <WrapPanel>
            <TextBox x:Name="resultBox" Margin="40" Width="320" Height="300"/>
            <TextBox x:Name="mapBox"  Width="320" Height="300"/>
        </WrapPanel>
        
    </Grid>
</Window>
